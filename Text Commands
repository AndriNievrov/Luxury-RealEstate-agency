        bot.send_message(message.chat.id, 'Ð£ Ð½Ð°Ñ Ð¿Ñ€Ð°Ñ†ÑŽÑ” ÐºÐ¾Ð¼Ð°Ð½Ð´Ð° Ð¿Ñ€Ð¾Ñ„ÐµÑÑ–Ð¾Ð½Ð°Ð»Ñ–Ð², ÑÐºÑ– Ð´Ð¾Ð¿Ð¾Ð¼Ð¾Ð¶ÑƒÑ‚ÑŒ Ð²Ð°Ð¼ Ð·Ð½Ð°Ð¹Ñ‚Ð¸ Ñ–Ð´ÐµÐ°Ð»ÑŒÐ½Ðµ Ð¶Ð¸Ñ‚Ð»Ð¾.')
    elif message.text == 'ðŸ’³ ÐžÑ„Ð¾Ñ€Ð¼Ð¸Ñ‚Ð¸ Ð·Ð°Ð¼Ð¾Ð²Ð»ÐµÐ½Ð½Ñ' or message.text == 'âœï¸ ÐÐ°Ð¿Ð¸ÑÐ°Ñ‚Ð¸ Ñ€Ð¾Ð·Ñ€Ð¾Ð±Ð½Ð¸ÐºÑƒ':
        webbrowser.open('https://t.me/foodeliveryy')
